{
  "buildCommand": null,
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    {
      "source": "/app",
      "destination": "/app/index.html"
    },
    {
      "source": "/app/(.*)",
      "destination": "/app/index.html"
    },
    {
      "source": "/upsell1",
      "destination": "/upsell1/index.html"
    },
    {
      "source": "/upsell2",
      "destination": "/upsell2/index.html"
    },
    {
      "source": "/thanks",
      "destination": "/thanks/index.html"
    },
    {
      "source": "/((?!app|upsell1|upsell2|thanks).*)",
      "destination": "/index.html"
    }
  ]
}